							for (var i = 0; i < carousels.length; i++) {
								str += '<h3>' + carousels[i].title + '</h3>';
								str += '<div class="carousel' + i + '"><div class="btn-left carousel-button"></div><div class="btn-right carousel-button"></div><ul>';
