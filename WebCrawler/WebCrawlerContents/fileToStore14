									str += '<li class="carousel-item"><a name="' + carousels[i].label + '-carousel-item-' + (j+1) + '-auction-' + auctionId + '" href="/auction/view?auctionId=' + auctionId + '&cm_re=gl-homepage-_-carousel-' + (i+1) + '-_-item-' + (j+1) + '"><div class="img-bin  previewable-lot" data-title="'+auctionTitle+'" name="'+auctionId+'"><img src="' + auctionImgUrl + '" alt="' + auctionTitle + '"/></div><div class="auction_title">' + auctionTitle + '</div></a></li>';
								}

								str += '</ul></div><div class="clear"></div>';
								/*if(i != carousels.length -1)
									str += '<hr/>';*/

							}

							$('#carouselContainer').html(str);
                            if($(".carousel0 li.carousel-item").length > 4){
                                $(".carousel0").jCarouselLite({
                                    btnNext: ".carousel0 .btn-right",
                                    btnPrev: ".carousel0 .btn-left",
                                    scroll : 4
                                });
                            } else {
                                $(".carousel0 .btn-right").remove();
                                $(".carousel0 .btn-left").remove();
                            }
                            if($(".carousel1 li.carousel-item").length > 4){
                                $(".carousel1").jCarouselLite({
