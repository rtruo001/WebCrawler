                                }
                                bannerArea = $('#bannerArea');
                            }
                            var banner = {
                                opacity : 0.2,
                                imgWidth : $('.bannerWrapper ul li').find('img').width(),
                                imgHeight : $('.bannerWrapper ul li').find('img').height()
                            };

                            var bannerCollectionStr = '';
                            var cgs = bannerArea.find('ul li');
                            for (i = 0; i < cgs.length; i++) {
                                lbl = $(cgs[i]).attr('name');
                                var collection = data.collections[lbl];
                                bannerCollectionStr += '<div class="bannerCollection" id="banner_' + lbl + '"><div class="bannerMenu-header">' + collection.title + '</div>';
                                bannerCollectionStr += '<div class="close-button"></div>';
                                bannerCollectionStr += '<ul>';
                                var size = collection.auctions.length > 7 ? 7 : collection.auctions.length ;
                                for (j = 0; j < size; j++) {
                                    var auc = collection.auctions[j];
                                    auctionTitle = auc.LOT_TITLE;
                                    auctionImgUrl = returnOriginalImage(auc.THUMBNAIL_URL)
                                    auctionImgUrl = auctionImgUrl.replace('gl_tiny_thumb', 'gl_carousel');
                                    auctionId = auc.AUCTION_ID;
                                    bannerCollectionStr += '<li><a href="/auction/view?auctionId=' + auctionId + '"><img class="previewable-lot" data-title="'+auctionTitle+'" name="' + auctionId + '" src="' + auctionImgUrl + '"/>';
                                    bannerCollectionStr += '<div class="banner-auc-title">' + auctionTitle + '</div>';
                                    /*bannerCollectionStr += '<div class="current-bid">$'+auc.current_bid+'</div>';*/
                                    bannerCollectionStr += '</a></li>';
                                }
                                bannerCollectionStr += '</ul></div>';
                            }
                            $('#bannerMenu').html(bannerCollectionStr);
//                            $('#bannerArea ul li').css({ 'width' : banner.imgWidth, 'height' : banner.imgHeight });
                            var imgSelected = false;
                            $('#bannerArea ul li').hover(function() {
                                $(this).find('img').addClass('active').css({ 'opacity' : 1});
                                $(this).siblings('li').find('img:not(.selected)').css({'opacity' : banner.opacity});
                            }, function() {
                                $(this).find('img').removeClass('active');
                            }).find('img').bind('click', function() {
                                var c = $(this);
                                var currentCollection = c.parents('li');
                                var parentCon = currentCollection.parent();
                                var curCollection = $('#bannerMenu #banner_' + currentCollection.attr('name'));
                                if (curCollection.is(':visible')) {
                                    curCollection.parent().slideUp();
                                    imgSelected = false;
                                    c.removeClass('selected')
                                    c.parents('li').siblings('li').find('img').css({'opacity' : 1});
                                    return false;
                                }
                                if (parentCon.find('img.selected').length != 0) {
                                    parentCon.find('img.selected').removeClass('selected').css({'opacity' : banner.opacity});
                                }
                                imgSelected = true;
                                c.addClass('selected');
                                $('#bannerMenu .bannerCollection').hide();

                                curCollection.show();
                                $('#bannerMenu').slideDown(500, function() {
                                    if (!curCollection.hasClass('truncated')) {
                                        curCollection.find('.banner-auc-title').autoEllipsisByWord('', {lines:3,showTitle:false});
                                        curCollection.addClass('truncated');
                                    }
                                });
                            });

                            $('#bannerMenu .close-button').bind('click', function() {
                                $(this).parent().parent().slideUp();
                                imgSelected = false;
                                var c = $('#bannerArea img.selected');
                                c.removeClass('selected')
                                c.parents('li').siblings('li').find('img').css({'opacity' : 1});
                            });

                            $('#bannerArea ul').bind('mouseleave', function() {
                                if (!imgSelected)
                                    $(this).find('img').css('opacity', 1);
                                else {
                                    $(this).find('img:not(.selected)').css('opacity', banner.opacity);
                                    $(this).find('img.selected').css('opacity', 1);
                                }
                            });
                        }
                        previewChanges();
                    }, 1000);

				});

		$(document).ready(function() {
			flag = true;
