					$(this).attr('href','http://'+ window.location.host+$(this).attr('href'));
				}
			});
			$('.footer ul.links a.link').each(function(){
				var c = $(this);
				if(!c.hasClass('not-needed')) {
					if(c.hasClass('secure')) {
						c.attr('href','https://'+ window.location.host+$(this).attr('href'));
					} else {
						c.attr('href','http://'+ window.location.host+$(this).attr('href'));
					}
				}
			});
		});
	
