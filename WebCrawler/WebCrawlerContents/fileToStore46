	Y.on("domready", function(){
		/* ============== Article show-hide ================== */
		Y.all('.art .mc').addClass('mch');
		Y.all('.art .rm').on( 'click', function( e )  {
			if(e.target.get("tagName")!="A"){var exid = Y.one("#" + (e.target.ancestor().get("href").split("#")[1]));}
			else {var exid = Y.one("#" + (e.target.get("href").split("#")[1]));}
			var cexi = exid.get("className");
			if(cexi.indexOf("mch") > 0) {exid.removeClass("mch");exid.addClass("mcs");}
			else {exid.addClass("mch");exid.removeClass("mcs");}
			e.preventDefault();
		});

		//acod
		Y.all(".art .acod").each(function(node){
			var acodHtml = node.getHTML();
			if(acodHtml){
				node.setHTML(acodHtml.replace(/</g,"&lt;").replace(/>/g,"&gt;"));
			}
		});
	});
	
});
